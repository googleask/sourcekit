{
  "manifest_version": 2,
  "name": "SourceKit",
  "version": "0.8.3",
  "incognito": "split",
  "icons": { 
    "128": "icons/icon_128.png",
    "16": "icons/icon_16.png"
  },
  "app": {
    "launch": {
      "local_path": "resources/application.html"
    }
  },
  "web_accessible_resources": [
    "resources/*",
    "src/*",
    "icons/*"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
	"tabs",
    "notifications",
    "http://*.dropbox.com/",
    "https://*.dropbox.com/"
  ]
}
